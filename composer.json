{
  "name": "arraypress/wp-register-admin-bar",
  "version": "1.0.0",
  "description": "A comprehensive WordPress admin bar management system with styling and node organization features",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-admin-bar/",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\WP\\Register\\": "src/"
    },
    "files": [
      "src/Utilities.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-register-admin-bar/issues",
    "source": "https://github.com/arraypress/wp-register-admin-bar"
  },
  "keywords": [
    "wordpress",
    "admin-bar",
    "menu",
    "nodes",
    "wp-admin",
    "toolbar",
    "navigation",
    "styling",
    "ui",
    "interface",
    "management"
  ]
}